{
  "name": "weather",
  "version": "0.1.0",
  "description": "A weather query CLI",
  "author": "Leonardo Perria",
  "scripts": {
    "start": "node dist/weather.js",
    "build": "npm run build-ts && npm run lint",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles",
    "build-ts": "tsc",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix"
  },
  "dependencies": {
    "async": "3.0.1",
    "bluebird": "3.5.5",
    "dotenv": "8.0.0",
    "lodash": "4.17.13"
  },
  "devDependencies": {
    "@types/async": "3.0.0",
    "@types/bluebird": "3.5.27",
    "@types/chai": "4.1.7",
    "@types/dotenv": "6.1.1",
    "@types/jest": "24.0.15",
    "@types/lodash": "4.14.134",
    "@types/node": "8.9.3",
    "@typescript-eslint/eslint-plugin": "1.12.0",
    "@typescript-eslint/parser": "1.12.0",
    "chai": "4.2.0",
    "eslint": "^5.0.0",
    "jest": "^24.8.0",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "typescript": "3.5.2",
    "acorn": "7.1.0"
  }
}
